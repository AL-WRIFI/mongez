{
    "name": "hassanzohdy/laravel-organizer",
    "description": "An organized structure for building a neat laravel application(s).",
    "keywords": ["laravel","organizer"],
    "license": "MIT",
    "version": "1.4.21",
    "require": {
        "php": ">=7",
        "illuminate/support": "5.6.x|5.7.x|5.8.x"
    },
    "authors": [
        {
            "name": "Hasan Zohdy",
            "email": "hassanzohdy@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "HZ\\Illuminate\\Organizer\\": "src/"
        },
        "files": [
            "src/HZ/Illuminate/Organizer/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HZ\\Illuminate\\Organizer\\Providers\\OrganizerServiceProvider"
            ]
        }
    }
}